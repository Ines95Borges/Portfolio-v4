{
  "name": "Portfolio",
  "version": "1.0.0",
  "description": "Inês's Borges Portfolio Website",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel sass server postcss",
    "sass": "sass --watch sass:dist/css",
    "server": "browser-sync start --server --files \"**/*.html js/**/*.js dist/css/**/*.css\"",
    "postcss": "postcss dist/css/main.css --use=autoprefixer --output=dist/final-css/main.css"
  },
  "keywords": [
    "Inês Borges",
    "Inês",
    "Borges",
    "Inês Borges's Portfolio"
  ],
  "author": "Inês Borges",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "sass": "^1.34.0"
  }
}
